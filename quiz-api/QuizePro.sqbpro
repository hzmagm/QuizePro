CREATE TABLE questions (
    id          INTEGER PRIMARY KEY AUTOINCREMENT,
    title       TEXT,
    position    INTEGER,
    text        TEXT,
    image       BLOB
);

CREATE TABLE possibleAnswers (
    id          INTEGER PRIMARY KEY AUTOINCREMENT,
    text        TEXT,
    isCorrect  TEXT,
    question_id INTEGER,
    FOREIGN KEY(question_id) REFERENCES questions(id)
);

